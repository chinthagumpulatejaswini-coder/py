a=input("Enter boy name:")
b=input("Enter girl name:")
a1=list(a)
b1=list(b)
for i in range(len(a1)):
    for j in range(len(b1)):
        if(a1[i]==b1[j]):
            a1[i]=2
            b1[j]=2
print(a1,b1)
c=0
for i in range(len(a1)):
    if(a1[i]!=2):
        c=c+1
for j in range(len(b1)):
    if(b1[j]!=2):
        c=c+1
print(c)

f=0
res=list("FLAMES")
for i in range(5):
    f=(f+(c-1))%len(res)
    res.pop(f)
print(res)


a=input("Enter the boy name:")
b=input("Enter the girl name:")
a1=list(a)
b1=list(b)
for i in range(len(a1)):
    for j in range(len(b1)):
        if(a1[i]==b1[j]):
            a1[i]=2
            b1[j]=2
print(a1,b1)
count=0
for i in a1:
    if(i!=2):
        count=count+1
for i in b1:
    if(i!=2):
        count=count+1
print(count)
f=0
res=list("flames")
for i in range(5):
    f=(f+(count-1))%len(res)
    res.pop(f)
print(res)
    
    
